{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "interlock_event",
  "type": "object",
  "required": ["interlock_id", "site_id", "asset_id", "ts", "state", "trigger"],
  "properties": {
    "interlock_id": { "type": "string" },
    "site_id": { "type": "string" },
    "asset_id": { "type": "string" },
    "ts": { "type": "string", "format": "date-time" },
    "state": { "type": "string", "enum": ["tripped", "reset"] },
    "trigger": { "type": "string", "maxLength": 200 },
    "notes": { "type": "string" }
  },
  "additionalProperties": false
}
